  "bugs": {
    "url": "https://github.com/ritterim/platform-icons/issues"
  },
  "homepage": "https://github.com/ritterim/platform-icons#readme",
  "devDependencies": {
    "fantasticon": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-copy": "^3.4.0",
    "svgo": "^3.0.2",
    "vite": "^3.2.5",
    "vite-plugin-banner": "^0.2.0",
    "vite-plugin-handlebars": "^1.6.0"
  }
}
